    }
  ],
  "LeftJoins": [
    {
      "Table": "Inventory",
      "Field1": "Vouchs.InvId",
      "Field2": "Inventory.Id"
    },
    {
      "Table": "InvStock",
      "Field1": "Vouchs.InvId",
      "Field2": "InvStock.Id"
    },
    {
      "Table":"UOM",
      "Field1":"InvStock.StockUOM",
      "Field2":"UOM.Id"
    },
    {
      "Table":"Locator as FromLocator",
      "Field1":"Vouchs.FromLocatorId",
      "Field2":"FromLocator.Id"
    },
    {
      "Table": "Locator as ToLocator",
