        "Table": "Locator",
        "Value": "Id",
        "Labels": [ "WhId", "Name" ]
      }
    },
    {
      "DbField": "ToLocator.Name"
    },
    {
      "DbField": "Vouchs.Batch"
    },
    {
      "DbField": "Vouchs.Num",
      "Type": "decimal"
    },
    {
      "DbField": "Vouchs.CNum",
      "Type": "decimal"
    },
    {
      "DbField": "Vouchs.Price",
      "Type": "decimal"
    },
    {
      "DbField": "Vouchs.Amount",
      "Type": "decimal"
    },
    {
      "DbField": "Vouchs.MadeDate",
      "GetFormatter": "yyyy-MM-dd",
      "SetFormatter": "yyyy-MM-dd"
    },
    {
